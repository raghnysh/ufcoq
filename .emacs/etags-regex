  -- Regular expressions for etags

  -- =================================================================
  -- Definition
  -- =================================================================

  -- Definition foo ...
  --
  -- \1 = foo

/[ \t]*Definition[ \t]+\([^ \t]+\)/\1/

  -- Local Definition foo ...
  --
  -- \1 = foo

/[ \t]*Local[ \t]+Definition[ \t]+\([^ \t]+\)/\1/

  -- =================================================================
  -- Theorem
  -- =================================================================

  -- Theorem foo ...
  --
  -- \1 = foo

/[ \t]*Theorem[ \t]+\([^ \t]+\)/\1/

  -- Local Theorem foo ...
  --
  -- \1 = foo

/[ \t]*Local[ \t]+Theorem[ \t]+\([^ \t]+\)/\1/

  -- =================================================================
  -- Proposition
  -- =================================================================

  -- Proposition foo ...
  --
  -- \1 = foo

/[ \t]*Proposition[ \t]+\([^ \t]+\)/\1/

  -- Local Proposition foo ...
  --
  -- \1 = foo

/[ \t]*Local[ \t]+Proposition[ \t]+\([^ \t]+\)/\1/

  -- =================================================================
  -- Lemma
  -- =================================================================

  -- Lemma foo ...
