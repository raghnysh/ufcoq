  -- Local Lemma foo ...
  --
  -- \1 = foo

/[ \t]*Local[ \t]+Lemma[ \t]+\([^ \t]+\)/\1/

  -- =================================================================
  -- Corollary
  -- =================================================================

  -- Corollary foo ...
