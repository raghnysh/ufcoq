  -- =================================================================
  -- Identity Coercion
  -- =================================================================

  -- Identity[ \t]+Coercion foo ...
  --
  -- \1 = foo
/[ \t]*Identity[ \t]+Coercion[ \t]+\([^ \t]+\)/\1/

  -- Local Identity[ \t]+Coercion foo ...
  --
  -- \1 = foo
/[ \t]*Local[ \t]+Identity[ \t]+Coercion[ \t]+\([^ \t]+\)/\1/

